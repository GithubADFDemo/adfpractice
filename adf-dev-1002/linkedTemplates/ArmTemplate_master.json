{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dev-1002"
		},
		"ADBLinkedService_properties_typeProperties_domain": {
			"type": "string",
			"defaultValue": "https://adb-2923826945884909.9.azuredatabricks.net"
		},
		"ADBLinkedService_properties_typeProperties_workspaceResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/aad39ad4-37cd-4c77-9cd9-0906efcae14a/resourceGroups/my-dev-rg/providers/Microsoft.Databricks/workspaces/adb-dev-1002"
		},
		"ADBLinkedService_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0406-054440-axr19ebc"
		},
		"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-dev-1002.vault.azure.net/"
		},
		"LS-ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cs210032001c77d1d5e.dfs.core.windows.net/"
		},
		"default_properties_kv_name_value": {
			"type": "string",
			"defaultValue": "https://kv-dev-1002.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dev-1002_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"default_properties_kv_name_value": {
						"value": "[parameters('default_properties_kv_name_value')]"
					},
					"ADBLinkedService_properties_typeProperties_domain": {
						"value": "[parameters('ADBLinkedService_properties_typeProperties_domain')]"
					},
					"ADBLinkedService_properties_typeProperties_workspaceResourceId": {
						"value": "[parameters('ADBLinkedService_properties_typeProperties_workspaceResourceId')]"
					},
					"ADBLinkedService_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ADBLinkedService_properties_typeProperties_existingClusterId')]"
					},
					"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_ls_properties_typeProperties_baseUrl')]"
					},
					"LS-ADLS_properties_typeProperties_url": {
						"value": "[parameters('LS-ADLS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}