{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dev-1003"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=myserver.windows.net;Initial Catalog=mytestdb;User ID=sqladmin"
		},
		"LS-ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS-ADLS'"
		},
		"ADBLinkedService_properties_typeProperties_accessToken": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVault_ls",
					"type": "LinkedServiceReference"
				},
				"secretName": "SQLPassword"
			}
		},
		"ADBLinkedService_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "sfsdgfdhfdshdfgystrytr"
		},
		"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-dev-1002.vault.azure.net/"
		},
		"AzureSqlDatabase1_properties_typeProperties_password": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVault_ls",
					"type": "LinkedServiceReference"
				},
				"secretName": "SQLPassword"
			}
		},
		"LS-ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://csg100320009c814a38.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dev-1003_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS-ADLS_accountKey": {
						"value": "[parameters('LS-ADLS_accountKey')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_ls_properties_typeProperties_baseUrl')]"
					},
					"LS-ADLS_properties_typeProperties_url": {
						"value": "[parameters('LS-ADLS_properties_typeProperties_url')]"
					},
					"ADBLinkedService_properties_typeProperties_accessToken": {
						"value": "[parameters('ADBLinkedService_properties_typeProperties_accessToken')]"
					},
					"ADBLinkedService_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ADBLinkedService_properties_typeProperties_existingClusterId')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_password": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_password')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}